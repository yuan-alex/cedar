{
  "name": "chatbot-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "prisma": {
    "seed": "bun run ./src/scripts/seed.ts"
  },
  "scripts": {
    "dev": "bun run --watch ./server/bun.ts",
    "start": "NODE_ENV=production bun run ./server/bun.ts",
    "build": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "preview": "vite preview",
    "format": "biome check . --write",
    "db:studio": "prisma studio --browser none",
    "db:generate": "prisma generate --no-hints",
    "db:migrate": "prisma generate --no-engine && prisma migrate deploy",
    "fetch:models": "bun run ./src/scripts/fetch-models.ts > ./server/utils/models.json"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.17",
    "@ai-sdk/deepinfra": "^1.0.18",
    "@ai-sdk/fireworks": "^1.0.18",
    "@ai-sdk/openai": "^2.0.32",
    "@ai-sdk/openai-compatible": "^1.0.18",
    "@ai-sdk/react": "^2.0.45",
    "@daveyplate/better-auth-ui": "^3.1.10",
    "@hono/zod-validator": "^0.7.3",
    "@lobehub/icons-static-svg": "^1.66.0",
    "@modelcontextprotocol/sdk": "^1.18.0",
    "@nanostores/persistent": "^1.1.0",
    "@nanostores/react": "^1.0.0",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-router": "^1.131.44",
    "@tanstack/react-router-devtools": "^1.131.44",
    "ai": "^5.0.45",
    "better-auth": "^1.3.11",
    "bun-plugin-tailwind": "^0.0.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "hono": "^4.9.7",
    "lucide-react": "^0.544.0",
    "nanostores": "^1.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-syntax-highlighter": "^15.6.6",
    "sonner": "^2.0.7",
    "streamdown": "^1.3.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.8",
    "use-stick-to-bottom": "^1.1.1",
    "yaml": "^2.8.1",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
