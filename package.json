{
  "name": "chatbot-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run dev:server\" \"bun run dev:client\"",
    "dev:server": "bun run --watch ./server/bun.ts",
    "dev:client": "vite --port 3000",
    "start": "bun run ./server/bun.ts",
    "build": "vite build",
    "build:vercel": "prisma generate --no-engine --no-hints && prisma migrate deploy && vite build && bun build ./api/index.ts --format=esm --minify --sourcemap --outdir=./dist/api",
    "preview": "vite preview",
    "format": "biome format . --write",
    "db:studio": "prisma studio --browser none",
    "db:generate": "prisma generate --no-hints",
    "db:migrate": "prisma generate --no-engine && prisma migrate deploy"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.15",
    "@ai-sdk/react": "^2.0.8",
    "@daveyplate/better-auth-ui": "^3.1.5",
    "@hono/node-server": "^1.18.1",
    "@hono/zod-validator": "^0.7.2",
    "@lobehub/icons-static-svg": "^1.62.0",
    "@modelcontextprotocol/sdk": "^1.17.3",
    "@nanostores/persistent": "^1.1.0",
    "@nanostores/react": "^1.0.0",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.84.2",
    "@tanstack/react-router": "^1.131.2",
    "@tanstack/react-router-devtools": "^1.131.2",
    "ai": "^5.0.34",
    "better-auth": "^1.3.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "hono": "^4.9.0",
    "katex": "^0.16.22",
    "lucide-react": "^0.539.0",
    "marked": "^16.1.2",
    "nanostores": "^1.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.6",
    "use-stick-to-bottom": "^1.1.1",
    "yaml": "^2.8.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.7",
    "@biomejs/biome": "^2.1.4",
