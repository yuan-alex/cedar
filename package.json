{
  "name": "chatbot-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run dev:server\" \"bun run dev:client\"",
    "dev:server": "bun run --watch ./server/bun.ts",
    "dev:client": "vite --port 3000",
    "start": "bun run ./server/bun.ts",
    "build": "vite build",
    "build:vercel": "prisma generate --no-engine --no-hints && prisma migrate deploy && vite build && bun build ./api/index.ts --format=esm --minify --sourcemap --outdir=./dist/api",
    "preview": "vite preview",
    "format": "biome format . --write",
    "db:studio": "prisma studio --browser none",
    "db:generate": "prisma generate --no-hints",
    "db:migrate": "prisma generate --no-engine && prisma migrate deploy"
  },
  "dependencies": {
    "@ai-sdk/openai-compatible": "^0.2.14",
    "@ai-sdk/react": "^1.2.12",
    "@clerk/backend": "^2.3.0",
    "@clerk/clerk-react": "^5.32.2",
    "@hono/clerk-auth": "^3.0.1",
    "@hono/node-server": "^1.14.4",
    "@hono/zod-validator": "^0.7.0",
    "@lobehub/icons": "^2.7.0",
    "@nanostores/persistent": "^1.0.0",
    "@nanostores/react": "^1.0.0",
    "@openrouter/ai-sdk-provider": "^0.7.2",
    "@prisma/client": "^6.10.1",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-router": "^1.123.0",
    "@tanstack/react-router-devtools": "^1.123.0",
    "ai": "^4.3.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "hono": "^4.8.3",
    "katex": "^0.16.22",
    "lucide-react": "^0.525.0",
    "marked": "^16.0.0",
    "nanostores": "^1.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.4",
    "use-stick-to-bottom": "^1.1.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@nanostores/logger": "^1.0.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@tanstack/router-plugin": "^1.123.0",
