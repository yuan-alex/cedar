{
  "name": "chatbot-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "prisma": {
    "seed": "bun run ./src/scripts/seed.ts"
  },
  "scripts": {
    "dev:server": "bun run --watch ./server/bun.ts",
    "dev:client": "bunx --bun vite dev",
    "start": "bun run ./server/bun.ts",
    "build": "bunx --bun vite build",
    "format": "biome check . --write",
    "db:studio": "prisma studio --browser none",
    "db:generate": "prisma generate --no-hints",
    "db:migrate": "prisma generate && prisma migrate deploy",
    "fetch:models": "bun run ./src/scripts/fetch-models.ts > ./server/utils/models.json"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.1",
