{
  "name": "chatbot-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "prisma": {
    "seed": "bun run ./src/scripts/seed.ts"
  },
  "scripts": {
    "dev": "concurrently \"bun run dev:server\" \"bun run dev:client\"",
    "dev:server": "bun run --watch ./server/bun.ts",
    "dev:client": "vite --port 3000",
    "start": "NODE_ENV=production bun run ./server/bun.ts",
    "build": "vite build",
    "preview": "vite preview",
    "format": "biome format . --write",
    "db:studio": "prisma studio --browser none",
    "db:generate": "prisma generate --no-hints",
    "db:migrate": "prisma generate --no-engine && prisma migrate deploy",
    "fetch:models": "bun run ./src/scripts/fetch-models.ts > ./server/utils/models.json"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.14",
    "@ai-sdk/deepinfra": "^1.0.15",
    "@ai-sdk/fireworks": "^1.0.15",
    "@ai-sdk/openai": "^2.0.27",
    "@ai-sdk/openai-compatible": "^1.0.15",
    "@ai-sdk/react": "^2.0.35",
    "@daveyplate/better-auth-ui": "^3.1.5",
    "@hono/node-server": "^1.19.1",
    "@hono/zod-validator": "^0.7.2",
    "@lobehub/icons-static-svg": "^1.65.0",
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@nanostores/persistent": "^1.1.0",
    "@nanostores/react": "^1.0.0",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-router": "^1.131.36",
    "@tanstack/react-router-devtools": "^1.131.36",
    "ai": "^5.0.35",
    "better-auth": "^1.3.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "hono": "^4.9.6",
    "katex": "^0.16.22",
    "lucide-react": "^0.544.0",
    "marked": "^16.2.1",
    "nanostores": "^1.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.6",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.8",
    "use-stick-to-bottom": "^1.1.1",
