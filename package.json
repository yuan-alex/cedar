    "@types/bun": "^1.2.22",
    "@types/node": "^24.5.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "lefthook": "^1.13.1",
    "prisma": "^6.16.2",
    "typescript": "^5.9.2"
  }
}
